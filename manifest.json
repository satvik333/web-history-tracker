{
    "manifest_version": 3,
    "name": "Kap Track",
    "version":"1.0.0",
    "description":"Keeps track of time spent on various domains",
    "icons": { 
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icons/icon16.png",
        "default_title": "Kap Track"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "storage",
        "webNavigation"
    ]
}
